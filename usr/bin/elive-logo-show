#!/bin/bash
SOURCE="$0"
source /usr/lib/elive-tools/functions

main(){
    c1="${el_c_n}"
    c2="${el_c_c2}"
    c3="${el_c_b2}"

    echo -e "${c2}
             *${c1}@${c2},,&(%%%..%*.
         ${c1}(@${c2}&%/##############((/${c1}*,
      ${c2}@${c1}@&${c2}#########${c1}*..../${c2}########%${c1}*..
    ${c2}@${c1}&${c2}#%%%%%.              ${c3},.${c1},${c2}%%%%%%.
  /%${c2}(%%%%.                      ${c1}(${c2}%%%%#.
 /${c1}*${c2}%%##,.                       .,%%###,
 ,####.   ,${c1}*${c2}#%${c1}#${c3}/,(/               ${c2}/${c1}#${c2}###,
((###/   ,,##########${c1}(${c3}/(#          ${c2}%####,
%#(((${c1}.   .${c1}./${c2}((((((((((((((${c1}(${c2}#/${c3}*..   ${c3}*.${c2}(((${c1}/
${c2}%#///${c1}.        ${c3}***${c2}.*/////////////
${c3}#${c2}#////*              ${c3}***${c2}.*/////.
 ${c3}(${c2}(*****                   ${c3}***
  ${c2},*****..
   ..${c1}*${c2}*****..                 *${c1}%${c2}/****.
     .,,*******,${c3},,../##${c2}(%&${c1}&${c2}#******${c1},${c2}.
        ,*${c1},${c2},,,,,,,,,,,,,,,,,,,${c1},${c2}..
            *//${c1}/,,${c2},,,,,,,${c1},..${c2}
"

}

#
#  MAIN
#
main "$@"

